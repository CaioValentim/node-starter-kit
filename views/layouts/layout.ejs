    <script>
        function validateNumber(event) {
            var key = window.event ? event.keyCode : event.which;
            if (event.keyCode === 8 || event.keyCode === 46) {
                return true;
            } else if (key < 48 || key > 57) {
                return false;
            } else {
                return true;
            }
        };

        function activeManu(){
            var url = window.location;

            let sortFunction = function(a, b){
                let ahref = a.href.replace(window.location.origin,"");
                let bhref = b.href.replace(window.location.origin,"");
                if(ahref < bhref){
                    return 1;
                }
                if(ahref > bhref){
                    return -1;
                }

                return ahref;

            }

            // for sidebar menu entirely but not cover treeview
            let elementsSorteds = $('ul.sidebar-menu a').filter(function() {
                return url.pathname.indexOf(this.href.replace(window.location.origin,"")) === 0;
            }).sort(sortFunction);

            if(elementsSorteds[0] && elementsSorteds[0].href){
                $().parent().addClass('active');
            }

            // for treeview
            let elementsSortedsThree = $('ul.treeview-menu a').filter(function() {
                return url.pathname.indexOf(this.href.replace(window.location.origin,"")) === 0;
            }).sort(sortFunction);

            if(elementsSortedsThree[0] && elementsSortedsThree[0].href){
                $(elementsSortedsThree[0]).parentsUntil(".sidebar-menu > .treeview-menu").addClass('active');
            }

        }

        $(document).ready(function() {

            function blockPage() {
                $.blockUI();
            }

            $("[type='submit']").on("click", blockPage);


            $('.diaMes').mask('00/00', {
                'translation': {
                    0: {
                        pattern: /[0-9*]/
                    }
                }
            });
            $('.ano').mask('0000', {
                'translation': {
                    0: {
                        pattern: /[0-9*]/
                    }
                }
            });
            $(".nascimento").inputmask("dd/mm/yyyy", {
                "placeholder": "dd/mm/aaaa"
            });
            $(".data").inputmask("dd/mm/yyyy", {
                "placeholder": "dd/mm/yyyy"
            });

            var maskBehavior = function(a) {
                    return 11 === a.replace(/\D/g, "").length ? "(00) 00000-0000" : "(00) 0000-00009"
                },
                options = {
                    onKeyPress: function(a, e, s, n) {
                        s.mask(maskBehavior.apply({}, arguments), n)
                    }
                };
            $(".telefone").mask(maskBehavior, options)

            $('.percent').mask('##0,00%', {
                reverse: true
            });

            $(".cpf").inputmask("999.999.999-99", {
                "placeholder": "   .   .   -  "
            });
            $(".cep").inputmask("99999-999", {
                "placeholder": "     -   "
            });
            $(".moeda").maskMoney({
                prefix: 'R$ ',
                allowNegative: true,
                thousands: '.',
                decimal: ',',
                affixesStay: false
            });
            $(".number").on("keypress", validateNumber);
            $('.firstLetterUp').keyup(function(evt) {
                var txt = $(this).val();
                $(this).val(txt.replace(/^(.)|\s(.)/g, function($1) {
                    return $1.toUpperCase();
                }));
            });

            activeManu();

            if ($.browser.webkit) {
                $('input').attr('autocomplete', 'new-password');
            }

        });
    </script>

